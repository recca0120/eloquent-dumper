{
    "name": "recca0120/eloquent-dumper",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "ext-pdo": "*",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
        "phpmyadmin/sql-parser": "^5.4"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^3.5|^4.1|^5.2|^6.2",
        "phpunit/phpunit": "^5.4|^6.1|^7.0|^8.0|^9.1",
        "symfony/var-dumper": "^3.2|^4.0|^5.4"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\EloquentDumper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\EloquentDumper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\EloquentDumper\\EloquentDumperServiceProvider"
            ]
        }
    }
}
