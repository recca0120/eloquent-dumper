{
    "name": "recca0120/eloquent-dumper",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "illuminate/database": "^5.1|^6.0|^7.0|^8.0",
        "phpmyadmin/sql-parser": "^4.6|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.3",
        "phpunit/phpunit": "^5.4|^6.1|^7.0|^8.0|^9.1"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\EloquentDumper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\EloquentDumper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\EloquentDumper\\EloquentDumperServiceProvider"
            ]
        }
    }
}
